{
	"name": "ADF_ADLS_Snowflake_Data_Flow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "adlsSalesDataSource"
				}
			],
			"sinks": [
				{
					"name": "SinkOfData"
				}
			],
			"transformations": [
				{
					"name": "FilterONSalesAmount"
				},
				{
					"name": "GroupBYYearIDMonthIDANDTotalSales"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ORDERNUMBER as short,",
				"          QUANTITYORDERED as short,",
				"          PRICEEACH as double,",
				"          ORDERLINENUMBER as short,",
				"          SALES as double,",
				"          ORDERDATE as timestamp,",
				"          STATUS as string,",
				"          QTR_ID as short,",
				"          MONTH_ID as short,",
				"          YEAR_ID as short",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false) ~> adlsSalesDataSource",
				"adlsSalesDataSource filter(SALES>=300) ~> FilterONSalesAmount",
				"FilterONSalesAmount aggregate(groupBy(YEAR_ID,",
				"          MONTH_ID),",
				"     TotalSales = sum(SALES)) ~> GroupBYYearIDMonthIDANDTotalSales",
				"GroupBYYearIDMonthIDANDTotalSales sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> SinkOfData"
			]
		}
	}
}